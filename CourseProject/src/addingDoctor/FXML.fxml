<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" fx:id="mainPanel" prefHeight="311.0" prefWidth="418.0" styleClass="mainFxmlClass" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="addingDoctor.FXMLController">
    <stylesheets>
        <URL value="@fxml.css" />
    </stylesheets>
   <children>
      <VBox prefHeight="326.0" prefWidth="432.0">
         <children>
            <TextField fx:id="docId" alignment="CENTER" onKeyReleased="#dataInputControl" promptText="Doctor ID number">
               <VBox.margin>
                  <Insets left="10.0" right="10.0" top="20.0" />
               </VBox.margin>
            </TextField>
            <TextField fx:id="docName" alignment="CENTER" layoutX="20.0" layoutY="30.0" promptText="Doctor Name">
               <VBox.margin>
                  <Insets left="10.0" right="10.0" top="20.0" />
               </VBox.margin>
            </TextField>
            <TextField fx:id="docSurname" alignment="CENTER" layoutX="10.0" layoutY="65.0" promptText="Doctor Surname">
               <VBox.margin>
                  <Insets left="10.0" right="10.0" top="20.0" />
               </VBox.margin>
            </TextField>
            <TextField fx:id="docDepartment" alignment="CENTER" layoutX="20.0" layoutY="120.0" promptText=" Doctor Department">
               <VBox.margin>
                  <Insets left="10.0" right="10.0" top="20.0" />
               </VBox.margin>
            </TextField>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Button fx:id="addbutton" mnemonicParsing="false" onAction="#addDoctor" prefHeight="77.0" prefWidth="196.0" text="add doctor">
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="cancelbutton" mnemonicParsing="false" onAction="#cancel" prefHeight="76.0" prefWidth="190.0" text="cancel">
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                  </Button>
               </children>
               <VBox.margin>
                  <Insets bottom="20.0" left="20.0" right="20.0" top="50.0" />
               </VBox.margin>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
