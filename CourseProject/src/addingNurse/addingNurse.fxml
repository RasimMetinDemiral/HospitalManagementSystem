<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" fx:id="mainPanel" prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="addingNurse.AddingNurseController">
    <stylesheets>
        <URL value="@addingnurse.css" />
    </stylesheets>
   <children>
      <VBox prefHeight="400.0" prefWidth="600.0">
         <children>
            <TextField fx:id="nurseId" alignment="CENTER" onKeyReleased="#dataInputControl" promptText="Nurse ID number">
               <VBox.margin>
                  <Insets left="10.0" right="10.0" top="20.0" />
               </VBox.margin>
            </TextField>
            <TextField fx:id="nurseName" alignment="CENTER" layoutX="20.0" layoutY="30.0" promptText="Nurse Name">
               <VBox.margin>
                  <Insets left="10.0" right="10.0" top="20.0" />
               </VBox.margin>
            </TextField>
            <TextField fx:id="nurseSurname" alignment="CENTER" layoutX="10.0" layoutY="65.0" promptText="Nurse Surname">
               <VBox.margin>
                  <Insets left="10.0" right="10.0" top="20.0" />
               </VBox.margin>
            </TextField>
            <TextField fx:id="nurseExperience" alignment="CENTER" layoutX="20.0" layoutY="120.0" onKeyReleased="#dataInputControlNurse" promptText=" Nurse Experience">
               <VBox.margin>
                  <Insets left="10.0" right="10.0" top="20.0" />
               </VBox.margin>
            </TextField>
            <HBox prefHeight="76.0" prefWidth="505.0">
               <children>
                  <Button fx:id="addbutton" mnemonicParsing="false" onAction="#addNurse" prefHeight="76.0" prefWidth="320.0" text="add Nurse">
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="cancelbutton" mnemonicParsing="false" onAction="#cancel" prefHeight="76.0" prefWidth="314.0" text="cancel">
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                  </Button>
               </children>
               <VBox.margin>
                  <Insets bottom="20.0" left="20.0" right="20.0" top="50.0" />
               </VBox.margin>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
